{
  "name": "phf",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "crypto": "^1.0.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "parallelshell \"watch 'npm run build:minify' ./src/css/\" \"react-scripts start\"",
    "build": "npm run build:css && npm run build:minify && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "mutations": "immutable-css src/tachyons.css --strict",
    "build:css": "tachyons src/css/tachyons.css > public/tachyons.css",
    "build:minify": "tachyons src/css/tachyons.css -m > public/tachyons.min.css"
  },
  "devDependencies": {
    "http": "0.0.0",
    "parallelshell": "3.0.2",
    "tachyons-cli": "^1.3.0",
    "watch": "1.0.2"
  }
